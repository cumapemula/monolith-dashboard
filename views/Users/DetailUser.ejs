    <!-- Table User -->
    <div
      class="container-list d-flex flex-column justify-content-center align-items-center"
    >
      <% if (row.length == 0) { %>
      <h1 class="text-uppercase fs-3 fw-bold">data tidak ditemukan</h1>
      <% } else { %>
      <div class="container-table w-75 rounded-3">
        <table class="table">
          <thead class="sticky-top">
            <tr>
              <% column.forEach(columns => { %>
              <th class="text-center text-uppercase fw-bold"><%= columns %></th>
              <% }) %>
            </tr>
          </thead>
          <tbody>
            <% row.forEach(rows => { %>
            <tr>
              <td class="text-center"><%= rows.id %></td>
              <td class="text-center"><%= rows.username %></td>
              <td class="text-center text-capitalize"><%= rows.role %></td>
              <td class="text-center text-capitalize">
                <%= rows['first name'] %>
              </td>
              <td class="text-center text-capitalize">
                <%= rows['last name'] %>
              </td>
              <td class="text-center text-capitalize"><%= rows.hobby %></td>
              <td class="text-center text-capitalize">
                <%= rows['game id'] %>
              </td>
              <td class="text-center text-capitalize">
                <%= rows['game name'] %>
              </td>
              <td class="text-center text-capitalize"><%= rows.win %></td>
              <td class="text-center text-capitalize"><%= rows.lose %></td>
              <td class="text-center text-capitalize">
                <%= rows.draw %>
              </td>
              <td class="text-center text-capitalize"><%= rows['total score'] %></td>
            </tr>
            <% }) %>
          </tbody>
        </table>
      </div>
      <% } %>
      <div class="btn-update mt-5">
        <a href="/dashboard/users/<%= req %>/account" class="btn btn-primary badge p-2 mx-2 border-0 text-decoration-none text-uppercase">update account</a>
        <a href="/dashboard/users/<%= req %>/biodata" class="btn btn-primary badge p-2 mx-2 border-0 text-decoration-none text-uppercase">update biodata</a>
        <a href="/dashboard/users/<%= req %>/score" class="btn btn-primary badge p-2 mx-2 border-0 text-decoration-none text-uppercase">update score</a>
        <a href="/dashboard/games" class="btn btn-primary badge p-2 mx-2 border-0 text-decoration-none text-uppercase">add new games</a>
      </div>
    </div>